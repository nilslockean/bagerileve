---
type Props = {
  feedId: string;
};

const { feedId } = Astro.props;
---

<behold-widget feed-id={feedId}></behold-widget>
<script>
  (() => {
    const d = document,
      s = d.createElement("script");
    s.type = "module";
    s.src = "https://w.behold.so/widget.js";
    d.head.append(s);
  })();
</script>
