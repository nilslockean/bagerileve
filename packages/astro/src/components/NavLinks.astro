---
import type { NavLink } from "@lib/types/NavLink";
import Link from "./Link.astro";

type Props = {
  links: NavLink[];
  class?: string;
};

const { links, class: className } = Astro.props;
---

{
  links.map((link) => (
    <Link href={link.path} current={link.current} class:list={[className]}>
      {link.label}
    </Link>
  ))
}
