---
import type { NavLink } from "@lib/types/NavLink";
import { cn } from "@lib/cn";

type Props = {
  links: NavLink[];
  class?: string;
};

const { links, class: className } = Astro.props;
---

{
  links.map((link) => (
    <a
      href={link.path}
      aria-current={link.current}
      class={cn(link.className, className)}
    >
      {link.label}
    </a>
  ))
}
